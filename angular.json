{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "asctb-reporter": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/asctb-reporter",
      "sourceRoot": "projects/asctb-reporter/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/asctb-reporter",
            "index": "projects/asctb-reporter/src/index.html",
            "main": "projects/asctb-reporter/src/main.ts",
            "polyfills": "projects/asctb-reporter/src/polyfills.ts",
            "tsConfig": "projects/asctb-reporter/tsconfig.app.json",
            "assets": [
              "projects/asctb-reporter/src/favicon.ico",
              "projects/asctb-reporter/src/assets",
              "projects/asctb-reporter/src/_redirects"
            ],
            "styles": [
              "projects/asctb-reporter/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/marked/marked.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/asctb-reporter/src/environments/environment.ts",
                  "with": "projects/asctb-reporter/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "projects/asctb-reporter/src/environments/environment.ts",
                  "with": "projects/asctb-reporter/src/environments/environment.stage.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "asctb-reporter:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "asctb-reporter:build:production"
            },
            "staging": {
              "browserTarget": "asctb-reporter:build:staging"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "asctb-reporter:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/asctb-reporter/src/test.ts",
            "polyfills": "projects/asctb-reporter/src/polyfills.ts",
            "tsConfig": "projects/asctb-reporter/tsconfig.spec.json",
            "karmaConfig": "projects/asctb-reporter/karma.conf.js",
            "assets": [
              "projects/asctb-reporter/src/favicon.ico",
              "projects/asctb-reporter/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/asctb-reporter/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/asctb-reporter/**/*.ts",
              "projects/asctb-reporter/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/asctb-reporter/e2e/protractor.conf.js",
            "devServerTarget": "asctb-reporter:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "asctb-reporter:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
